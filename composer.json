{
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "push": "vendor/bin/dep push",
    "publish": "vendor/bin/dep publish",
    "deploy": "vendor/bin/dep deploy",
    "unlock": "vendor/bin/dep deploy:unlock",
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
  },
  "autoload": {
    "psr-4": {
      "AugmentedSteam\\Server\\": "src"
    },
    "classmap": [
      "lib/LightOpenID/openid.php"
    ]
  },
  "require": {
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "itad/config": "^0.1",
    "itad/db": "^0.1",
    "itad/twitch-api": "^0.1",
    "league/route": "^5.1",
    "guzzlehttp/guzzle": "^7.4",
    "monolog/monolog": "^2.3",
    "laminas/laminas-diactoros": "^2.8",
    "laminas/laminas-httphandlerrunner": "^2.1",
    "php-di/php-di": "^6.3",
    "nette/schema": "^1.2",
    "league/uri": "^6.5",
    "league/uri-components": "^2.4"
  },
  "require-dev": {
    "deployer/deployer": "^7.0@rc",
    "phpstan/phpstan": "^1.4",
    "filp/whoops": "^2.14"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:IsThereAnyDeal/Config.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:IsThereAnyDeal/Database.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:IsThereAnyDeal/TwitchApi.git"
    }
  ]
}
